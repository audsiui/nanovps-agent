{
  "name": "nanovps-agent",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run src/index.ts",
    "clean": "rm -rf dist",
    "build": "bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:win-x64",
    "build:linux-x64": "bun build --compile --target=bun-linux-x64 --outfile=dist/monitor-agent-linux-amd64 src/index.ts",
    "build:linux-arm64": "bun build --compile --target=bun-linux-arm64 --outfile=dist/monitor-agent-linux-arm64 src/index.ts",
    "build:win-x64": "bun build --compile --target=bun-windows-x64 --outfile=dist/monitor-agent-windows-amd64.exe src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "systeminformation": "^5.30.6"
  }
}